{
  "rules": {
    "react/prop-types": "off",
        "no-unused-vars": "off",
        "no-constant-condition": "off",
        "no-empty": "off"
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}
